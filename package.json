{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A discord.js application",
  "main": "./source/desktop.js",
  "repository": "https://github.com/BBpezsgo/DiscordBot",
  "scripts": {
    "test": "node ./source/tests/test.js",
    "build": "tsc --project tsconfig.json",
    "start": "npm run build && node --enable-source-maps dist/desktop.js",
    "start-mobile": "node ./source/mobile",
    "start-desktop": "node ./source/desktop",
    "start-desktop-startup": "node ./source/desktop.js invisible system",
    "start-desktop-user": "cd source && node desktop.js visible user",
    "start-raspberry": "node ./source/raspberry"
  },
  "dependencies": {
    "@discordjs/voice": "latest",
    "adm-zip": "latest",
    "asciichart": "latest",
    "axios": "latest",
    "canvas": "^2.10.2",
    "csv": "^6.2.2",
    "discord.js": "latest",
    "express": "latest",
    "express-formidable": "^1.2.0",
    "express-handlebars": "latest",
    "ffmpeg": "latest",
    "ffmpeg-static": "latest",
    "fluent-ffmpeg": "latest",
    "fs": "latest",
    "jsdom": "latest",
    "moonphase-js": "latest",
    "nodejs-weather-app": "1.1.0",
    "suncalc": "^1.9.0",
    "weather-js": "latest",
    "xml2js": "^0.4.23",
    "ytdl-core": "latest"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5"
  }
}
