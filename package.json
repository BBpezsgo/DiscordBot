{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A discord.js application",
  "main": "./source/desktop.js",
  "repository": "https://github.com/BBpezsgo/DiscordBot",
  "scripts": {
    "test": "node ./source/tests/test.js",
    "build": "tsc --project tsconfig.json",
    "desktop": "npm run build && node --enable-source-maps dist/desktop.js",
    "mobile": "npm run build && node --enable-source-maps dist/mobile.js",
    "start-mobile": "cd source && node mobile.js",
    "start-desktop-startup": "cd source && node desktop.js invisible system",
    "start-desktop-user": "cd source && node desktop.js visible user",
    "start-raspberry": "node ./source/raspberry"
  },
  "dependencies": {
    "@discordjs/voice": "^0.13.0",
    "adm-zip": "^0.5.9",
    "asciichart": "^1.5.25",
    "axios": "^1.1.3",
    "canvas": "^2.10.2",
    "csv": "^6.2.2",
    "discord.js": "^14.6.0",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-handlebars": "^6.0.6",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "latest",
    "jsdom": "^20.0.1",
    "moonphase-js": "^0.2.0",
    "nodejs-weather-app": "1.1.0",
    "suncalc": "^1.9.0",
    "weather-js": "^2.0.0",
    "xml2js": "^0.4.23",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5"
  }
}
