<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
    <link href="/dynaform/css_main.css" rel="stylesheet" type="text/css">
    <link href="/dynaform/tree-default.css" rel="stylesheet" type="text/css">
    <script language="javascript" src="/dynaform/common.js" type="text/javascript"></script>
    <script language="javascript" src="/dynaform/custom.js" type="text/javascript"></script>
    <script language="javascript" src="/dynaform/tree-default.js" type="text/javascript"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
</head>

<body onload="setTagStr(document,'ntw_status'); resize(this);" onresize="resize(this);" marginwidth="0"
    marginheight="0">
    <table id="autoWidth" style="width: 598px;">
        <tbody>
            <tr>
                <td class="h1" colspan="3" id="t_title">Database</td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
				<td class="Item" id="t_mac_addr" name="t_mac_addr">Backup folder path:</td>
				<td colspan="2">
					{{info.backupFolder}}
				</td>
            </tr>
            <tr>
				<td class="Item" id="t_mac_addr" name="t_mac_addr">Folder path:</td>
				<td colspan="2">
					{{info.folder}}
				</td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">
                    BOT's database
                </td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <ul id="myUL">
                        <li>
                            <span class="data-number">Day: </span>
                            <b>{{bot.day}}</b>
                        </li>
                    </ul>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">
                    Market
                </td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <ul id="myUL">
                        <li>
                            <span class="data-number">Day: </span>
                            <b>{{market.day}}</b>
                        </li>
                        <li><span class="caret caret-down">Prices</span>
                            <ul class="nested active">
                                <li>
                                    <span class="data-number">token: </span>
                                    <b>{{market.prices.token}}</b>
                                </li>
                                <li>
                                    <span class="data-number">coupon: </span>
                                    <b>{{market.prices.coupon}}</b>
                                </li>
                                <li>
                                    <span class="data-number">jewel: </span>
                                    <b>{{market.prices.jewel}}</b>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">Users</td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <table class="datas" border="1" align="center" width="698" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr>
                                <td class="ListM">
                                    <b>Avatar</b>
                                </td>
                                <td class="ListM" style="width: 100%;">
                                    <b>Username</b>
                                </td>
                                <td class="ListM">
                                    <b>ID</b>
                                </td>
                            </tr>
                            {{#each users}}
                            <tr>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    <div class="tooltip">
                                        <img src="{{this.avatarUrlSmall}}" width="16" height="16" title="Avatar">
                                        <span class="tooltiptext">
                                            <img src="{{this.avatarUrlLarge}}">
                                        </span>
                                    </div>
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    {{this.name}}
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    {{this.id}}
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <form action="/userRpm/Database/Search" method="post"
                        onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%'; setTimeout(() => {window.location.reload()}, 1000);">
                        <input type="text" name="id" value="{{../this.id}}" placeholder="User ID" required>
                        <input type="submit" value="Search">
                        <span style="font-weight: bold; color: red;">{{searchError}}</span>
                    </form>
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>