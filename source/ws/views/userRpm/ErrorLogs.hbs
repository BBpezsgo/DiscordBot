<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
    <link href="/dynaform/css-vars.css" rel="stylesheet" type="text/css">
    <link href="/dynaform/css_main.css" rel="stylesheet" type="text/css">
    <link href="/dynaform/tree-errors.css" rel="stylesheet" type="text/css">
    <script language="javascript" src="/dynaform/common.js" type="text/javascript"></script>
    <script language="javascript" src="/dynaform/tree-default.js" type="text/javascript"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

</head>

<body onload="LoadHelp(window); resize(this);" onresize="resize(this);" marginwidth="0"
    marginheight="0">
    <button class="show-hide-part-button" onclick="parent.ToggleHelpPanel(this)"><</button>

   <table id="autoWidth" style="width: 598px;">
        <tbody>
            <tr>
                <td class="h1" colspan="3">Error Log</td>
            </tr>
            <tr>
                <td style="padding-top: 20px;">
                    <form action="/userRpm/Log/Clear" method="post" style="margin: 0;"
                        onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%'; setTimeout(() => {window.location.reload()}, 1000);">
                        <input type="submit" value="Clear log">
                    </form>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td>
                    <ul id="myUL">
                        {{#each errors}}
                        <li style="margin-bottom: 10px;">
                            {{#if isCrash}}
                            <span class="caret crash" style="font-weight: bold;">{{type}}: </span>
                            {{else}}
                            <span class="caret error" style="font-weight: bold;">{{type}}: </span>
                            {{/if}}
                            {{title}}
                            <ul class="nested">
                                {{#each info}}
                                {{this}}<br>
                                {{/each}}
                                {{#each stack}}
                                <li>
                                    <p style="display: inline;">at</p> <p style="display: inline;">{{this.raw}}</p>
                                </li>
                                {{/each}}
                            </ul>
                        </li>
                        {{/each}}
                    </ul>

                    <ul id="myUL">
                        {{#each warnings}}
                        <li style="margin-bottom: 10px;">
                            <span class="caret warning" style="font-weight: bold;">{{type}}: </span>
                            {{title}}
                            <ul class="nested">
                                <li>
                                    {{info}}
                                </li>
                            </ul>
                        </li>
                        {{/each}}
                    </ul>
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>