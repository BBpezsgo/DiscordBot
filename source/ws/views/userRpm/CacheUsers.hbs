<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
	<link href="/dynaform/css-vars.css" rel="stylesheet" type="text/css">
    <link href="/dynaform/css_main.css" rel="stylesheet" type="text/css">
    <script language="javascript" src="/dynaform/common.js" type="text/javascript"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
</head>

<body onload="LoadHelp(window); resize(this);" onresize="resize(this);" marginwidth="0"
    marginheight="0">
   	<button class="show-hide-part-button" onclick="parent.ToggleHelpPanel(this)"><</button>

    <table id="autoWidth" style="width: 598px;">
        <tbody>
            <tr>
                <td class="h1" colspan="3">Cache</td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3">Users</td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <table class="datas" border="1" align="center" width="698" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr>
                                <td class="ListM">
                                    <b>Avatar</b>
                                </td>
                                <td class="ListM" style="width: 100%;">
                                    <b>Username</b>
                                </td>
                                <td class="ListM">
                                    <b>ID</b>
                                </td>
                                <td class="ListM">
                                    <b>Created At</b>
                                </td>
                                <td class="ListM">
                                    <b>Discriminator</b>
                                </td>
                                <td class="ListM">
                                    <b>Accent Color</b>
                                </td>
                                <td class="ListM">
                                    <b>Hash</b>
                                </td>
                            </tr>
                            {{#each users}}
                            <tr>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    <img src="{{this.defaultAvatarUrl}}" width="16" height="16" title="Default avatar">
                                    
                                    <div class="tooltip">
                                        <img src="{{this.avatarUrlSmall}}" width="16" height="16" title="Avatar">
                                        <span class="tooltiptext">
                                            <img src="{{this.avatarUrlBig}}">
                                        </span>
                                    </div>
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    {{this.username}}
                                    <span class="tag-{{this.bot}}">BOT</span>
                                    <span class="tag-{{this.system}}">System</span>
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    {{this.id}}
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    {{this.createdAt}}
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    #{{this.discriminator}}
                                </td>
                                <td style="padding:0 2 0 2;word-break:break-all">
                                    <div style="
                                        width: 16px;
                                        height: 16px;
                                        border: 1px solid black;
                                        border-radius: 3px;
                                        background-color: {{this.hexAccentColor}};                                            
                                    ">
                                    </div>
                                </td>
                                <td>                                    
                                    <img width="16" height="16" src="/images/bool-{{this.haveHash}}.png" style="transform: translateY(3px);">
                                    <input type="text" value="{{this.hash}}" readonly>
                                    <form action="/userRpm/GenerateHash" method="post"
                                        style="margin: 0; display: inline-block;"
                                        onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%'; setTimeout(() => {window.location.reload()}, 1000);">
                                        <input type="text" name="id" value="{{this.id}}" style="display: none;">
                                        <input type="submit" value="Generate">
                                    </form>
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>