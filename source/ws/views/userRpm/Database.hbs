<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
    <link href="/dynaform/css_main.css" rel="stylesheet" type="text/css">
    <link href="/dynaform/tree-default.css" rel="stylesheet" type="text/css">
    <script language="javascript" src="/dynaform/common.js" type="text/javascript"></script>
    <script language="javascript" src="/dynaform/custom.js" type="text/javascript"></script>
    <script language="javascript" src="/dynaform/tree-default.js" type="text/javascript"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
</head>

<body onload="setTagStr(document,'ntw_status'); resize(this);" onresize="resize(this);" marginwidth="0"
    marginheight="0">
    <table id="autoWidth" style="width: 598px;">
        <tbody>
            <tr>
                <td class="h1" colspan="3" id="t_title">Database</td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
				<td width="25%" class="Item" id="t_mac_addr" name="t_mac_addr">Backup folder path:</td>
				<td colspan="2">
					{{info.backupFolder}}
				</td>
            </tr>
            <tr>
				<td width="25%" class="Item" id="t_mac_addr" name="t_mac_addr">Folder path:</td>
				<td colspan="2">
					{{info.folder}}
				</td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">
                    BOT's database
                </td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <ul id="myUL">
                        <li>
                            <span class="data-number">Day: </span>
                            <b>{{bot.day}}</b>
                        </li>
                    </ul>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">
                    Market
                </td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <ul id="myUL">
                        <li>
                            <span class="data-number">Day: </span>
                            <b>{{market.day}}</b>
                        </li>
                        <li><span class="caret caret-down">Prices</span>
                            <ul class="nested active">
                                <li>
                                    <span class="data-number">token: </span>
                                    <b>{{market.prices.token}}</b>
                                </li>
                                <li>
                                    <span class="data-number">coupon: </span>
                                    <b>{{market.prices.coupon}}</b>
                                </li>
                                <li>
                                    <span class="data-number">jewel: </span>
                                    <b>{{market.prices.jewel}}</b>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </td>
            </tr>
            <tr>
                <td class="blue" colspan="3"></td>
            </tr>
            <tr>
                <td class="h2" colspan="3" id="t_lan">
                    <div class="tooltip" style="transform: translateY(5px);">
                        <img src="{{user.avatarUrlSmall}}" width="32" height="32" title="Avatar">
                        <span class="tooltiptext">
                            <img src="{{user.avatarUrlLarge}}">
                        </span>
                    </div>
                    {{user.name}}'s database
                    <form action="/userRpm/Database/Back" method="post"
                        style="display: inline-block; margin-left: 30px;"
                        onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%'; setTimeout(() => {window.location.reload()}, 1000);">
                        <input type="submit" value="Back">
                    </form>
                </td>
            </tr>
            <tr>
                <td width="698" align="center">
                    <ul id="myUL">
                        <li><span class="caret caret-down">Basic</span>
                            <ul class="nested active">
                                <form action="/userRpm/Database/Modify/Basic" method="post" onsubmit="
                                    this.style.pointerEvents='none';
                                    this.style.opacity='20%';
                                    setTimeout(() => {window.location.reload()}, 1000);
                                    ">
                                    <li>
                                        <span class="data-number">Score: </span>
                                        <input name="score" type="number" value="{{userDatabase.score}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number"> Money: </span>
                                        <input name="money" type="number" value="{{userDatabase.money}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number"> Day: </span>
                                        <input name="day" type="number" value="{{userDatabase.day}}" required>
                                    </li>
                                    <li>
                                        <span class="data-string"> color: </span>
                                        <div style="
                                            display: inline-block;
                                            transform: translateY(5px);
                                            width: 16px;
                                            height: 16px;
                                            border: 1px solid black;
                                            border-radius: 3px;
                                            background-color: {{userDatabase.color}};                                            
                                        ">
                                        </div>
                                    </li>
                                    <li>
                                        <span class="data-bool">privateCommands: </span>
                                        <b>{{userDatabase.privateCommands}}</b>
                                    </li>
                                    <div style="height: 7px;"></div>
                                    <input type="submit" value="Modify">
                                    <input type="reset" value="Reset">
                                </form>
                            </ul>
                        </li>
                        <li><span class="caret caret-down">Backpack</span>
                            <ul class="nested active">
                                <form action="/userRpm/Database/Modify/Backpack" method="post" onsubmit="
                                    this.style.pointerEvents='none';
                                    this.style.opacity='20%';
                                    setTimeout(() => {window.location.reload()}, 1000);
                                    ">
                                    <li>
                                        <span class="data-number">crates: </span>
                                        <input name="crates" type="number" value="{{userDatabase.crates}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">gifts: </span>
                                        <input name="gifts" type="number" value="{{userDatabase.gifts}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">getGift: </span>
                                        <input name="getGift" type="number" value="{{userDatabase.getGift}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">tickets: </span>
                                        <input name="tickets" type="number" value="{{userDatabase.tickets}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">quizTokens: </span>
                                        <input name="quizTokens" type="number" value="{{userDatabase.quizTokens}}"
                                            required>
                                    </li>
                                    <li><span class="caret">Lucky cards</span>
                                        <ul class="nested">
                                            <li>
                                                <span class="data-number">small: </span>
                                                <input name="luckyCardsSmall" type="number"
                                                    value="{{userDatabase.luckyCards.small}}" required>
                                            </li>
                                            <li>
                                                <span class="data-number">medium: </span>
                                                <input name="luckyCardsMedium" type="number"
                                                    value="{{userDatabase.luckyCards.medium}}" required>
                                            </li>
                                            <li>
                                                <span class="data-number">getGift: </span>
                                                <input name="luckyCardsLarge" type="number"
                                                    value="{{userDatabase.luckyCards.large}}" required>
                                            </li>
                                        </ul>
                                    </li>
                                    <div style="height: 7px;"></div>
                                    <input type="submit" value="Modify">
                                    <input type="reset" value="Reset">
                                </form>
                            </ul>
                        </li>
                        <li><span class="caret">Business</span>
                            <ul class="nested">
                                <form action="/userRpm/Database/Modify/Business" method="post" onsubmit="
                                    this.style.pointerEvents='none';
                                    this.style.opacity='20%';
                                    setTimeout(() => {window.location.reload()}, 1000);
                                    ">
                                    <li><span class="data-number">businessIndex:
                                        </span><b>{{userDatabase.businessIndex}}</b></li>
                                    <li><span class="data-string">businessName:
                                        </span><b>{{userDatabase.businessName}}</b>
                                    </li>
                                    <li><span class="data-number">businessLevel:
                                        </span><b>{{userDatabase.businessLevel}}</b></li>
                                    <!--
                                    <input type="submit" value="Modify">
                                    <input type="reset" value="Reset">
                                    -->
                                </form>
                            </ul>
                        </li>
                        <li><span class="caret">Stickers</span>
                            <ul class="nested">
                                <form action="/userRpm/Database/Modify/Stickers" method="post" onsubmit="
                                    this.style.pointerEvents='none';
                                    this.style.opacity='20%';
                                    setTimeout(() => {window.location.reload()}, 1000);
                                    ">
                                    <li>
                                        <span class="data-number">stickersMeme: </span>
                                        <input name="stickersMeme" type="number" value="{{userDatabase.stickersMeme}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">stickersMusic: </span>
                                        <input name="stickersMusic" type="number" value="{{userDatabase.stickersMusic}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">stickersYoutube: </span>
                                        <input name="stickersYoutube" type="number" value="{{userDatabase.stickersYoutube}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">stickersMessage: </span>
                                        <input name="stickersMessage" type="number" value="{{userDatabase.stickersMessage}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">stickersCommand: </span>
                                        <input name="stickersCommand" type="number" value="{{userDatabase.stickersCommand}}" required>
                                    </li>
                                    <li>
                                        <span class="data-number">stickersTip: </span>
                                        <input name="stickersTip" type="number" value="{{userDatabase.stickersTip}}" required>
                                    </li>
                                    <div style="height: 7px;"></div>
                                    <input type="submit" value="Modify">
                                    <input type="reset" value="Reset">
                                </form>
                            </ul>
                        </li>
                        <li><span class="caret">Statistics</span>
                            <ul class="nested">
                                <li><span class="data-number">memes: </span><b>{{userDatabase.memes}}</b></li>
                                <li><span class="data-number">musics: </span><b>{{userDatabase.musics}}</b></li>
                                <li><span class="data-number">youtubevideos:
                                    </span><b>{{userDatabase.youtubevideos}}</b></li>
                                <li><span class="data-number">messages: </span><b>{{userDatabase.messages}}</b></li>
                                <li><span class="data-number">chars: </span><b>{{userDatabase.chars}}</b></li>
                                <li><span class="data-number">commands: </span><b>{{userDatabase.commands}}</b></li>
                            </ul>
                        </li>
                    </ul>
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>