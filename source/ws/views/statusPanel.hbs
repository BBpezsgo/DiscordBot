<div style="width: 100%; height: 100%;">
<img src="{{client.avatar}}" width="32px" height="32px" style="position: relative; top: 10px; left: 10px;">
<div
    style="border-radius: 8px; width: 16px; height: 16px; position:relative; top: 5px; left: -1px; background-color: #202225; display: inline-block; z-index: 9; margin: -8px">
</div>
<img src="{{client.status}}.png" width="10px" height="10px"
    style="position: relative; top: 10px; left: -9px; z-index: 10; border-radius: 50%;">
<div style="color: white; position: relative; left: -4px; top: -8px; display: inline-block;">
    {{client.username}}</div>
<div style="color: gray; position: relative; left: 50px; top: -8px; font-size: 12px;">
    #{{client.discriminator}}
</div>
<form action="/startBot" method="post" target="dummyframe"
    style="position: relative; top: -50px; height: 61px; right: 0; display: {{client.enStart}};"
     onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%';">
    <input style="display: none;" type="text" name="token" value="{{token}}">
    <button type="submit" class="StartStopButton">
        <i class="fa fa-play"></i>
    </button>
</form>
<form action="/stopBot" method="post" target="dummyframe"
    style="position: relative; top: -50px; height: 61px; right: 0; display: {{client.enStop}};"
     onsubmit="this.style.pointerEvents='none'; this.style.opacity='20%';">
    <input style="display: none;" type="text" name="token" value="{{token}}">
    <button type="submit" class="StartStopButton">
        <i class="fa fa-stop"></i>
    </button>
</form>
</div>