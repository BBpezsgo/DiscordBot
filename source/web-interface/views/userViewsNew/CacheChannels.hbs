<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
    <link href="/stylesNew/css-vars.css" rel="stylesheet" type="text/css">
    <link href="/stylesNew/css_main.css" rel="stylesheet" type="text/css">
    <link href="/stylesNew/tree.css" rel="stylesheet" type="text/css">
    <script language="javascript" src="/scripts/commonNew.js" type="text/javascript"></script>
    <script language="javascript" src="/scripts/tree.js" type="text/javascript"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <style>
        .material-icons {
            font-size: 1.2em;
            transform: translate(0px, 3px);
            user-select: none;
        }

        a:hover .material-icons::after {
            content: '';
            display: block;
            width: 1em;
            height: 1px;
            background-color: currentColor;
            transform: translateY(-2px);
        }

        img.badge {
            display: inline-block;
            transform: translateY(4px);
        }
    </style>
</head>

<body onload="LoadHelp(window); resize(this);" onresize="resize(this);" marginwidth="0" marginheight="0">
    <button class="show-hide-part-button" onclick="parent.ToggleHelpPanel(this)">&lt;</button>

	<h1>Cache</h1>

	<div class="container loading-effect">
		<h2>Channels</h2>
        <table class="datas" border="1" align="center" width="100%" cellspacing="0" cellpadding="0">
            <tbody>
                <tr>
                    <td class="ListM">
                        <div class="caret-all" title="Expand/Collapse All"></div>
                    </td>
                    <td class="ListM" style="width: 100%;">
                        <b>Name</b>
                    </td>
                    <td class="ListM">
                        <b>ID</b>
                    </td>
                    <td class="ListM">
                        <b title="Deletable">Delb.</b>
                    </td>
                    <td class="ListM">
                        <b title="Joinable">Join.</b>
                    </td>
                    <td class="ListM">
                        <b title="Speakable">Spek.</b>
                    </td>
                </tr>
                {{#each channels}}
                <tr>
                    <td style="padding: 0 2px; user-select: none;">
                        <img src="../imagesNew/{{typeUrl}}.png" width="16" height="16" title="{{typeText}}">
                    </td>
                    <td style="padding: 0 2px">
                        <span>{{name}}</span>
                        <a href="{{url}}" target="_blank" title="Channel Link"><i class="material-icons" style="margin-left: 4px;">link</i></a>
                        
                        {{#if viewable}}{{else}}
                        <img class="badge" src="../imagesNew/invisible.png" width="16" height="16" title="Invisible">
                        {{/if}}

                        {{#if manageable}}{{else}}
                        <img class="badge" src="../imagesNew/no-edit.png" width="16" height="16" title="Not Manageable">
                        {{/if}}
                    </td>
                    <td style="padding: 0 2px">
                        {{this.id}}
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.deletable}}.png">
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.joinable}}.png">
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.speakable}}.png">
                    </td>
                </tr>
                {{/each}}
                {{#each groups}}
                <tr>
                    <td style="padding: 0 2px; width: 16px; user-select: none;">
                        <div class="caret" id="{{this.id}}" title="Group Channel"></div>
                    </td>
                    <td style="padding: 0 2px">
                        <span>{{this.name}}</span>

                        {{#if manageable}}{{else}}
                        <img class="badge" src="../imagesNew/no-edit.png" width="16" height="16" title="Not Manageable">
                        {{/if}}
                    </td>
                    <td style="padding: 0 2px">
                        {{this.id}}
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.deletable}}.png">
                    </td>
                    <td colspan="2"></td>
                </tr>
                {{#each channels}}
                <tr class="nested {{this.parentId}}">
                    <td style="padding: 0 2px; user-select: none;">
                        <img src="../imagesNew/{{this.typeUrl}}.png" width="16" height="16"
                            title="{{this.typeText}}">
                    </td>
                    <td style="padding: 0 2px">
                        <span>{{name}}</span>
                        <a href="{{url}}" target="_blank" title="Channel Link"><i class="material-icons" style="margin-left: 4px;">link</i></a>
                        {{#if nsfw}}<span class="tag">NSFW</span>{{/if}}

                        {{#if viewable}}{{else}}
                        <img class="badge" src="../imagesNew/invisible.png" width="16" height="16" title="Invisible">
                        {{/if}}

                        {{#if manageable}}{{else}}
                        <img class="badge" src="../imagesNew/no-edit.png" width="16" height="16" title="Not Manageable">
                        {{/if}}
                    </td>
                    <td style="padding: 0 2px">
                        {{this.id}}
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.deletable}}.png">
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.joinable}}.png">
                    </td>
                    <td style="padding: 0 2px">
                        <img width="16" height="16" src="/imagesNew/bool-{{this.speakable}}.png">
                    </td>
                </tr>
                {{/each}}
                {{/each}}
            </tbody>
        </table>
	</div>
    
    <script src="../scripts/loadEffect.js"></script>
</body>

</html>