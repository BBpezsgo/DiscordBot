                    <td colspan="2">
                        {{process.chrome}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Context ID:</td>
                    <td colspan="2">
                        {{process.contextId}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Context isolated:</td>
                    <td colspan="2">
                        <img width="16" height="16" src="/imagesNew/bool-{{process.contextIsolated}}.png">
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Default app:</td>
                    <td colspan="2">
                        <img width="16" height="16" src="/imagesNew/bool-force-{{process.defaultApp}}.png">
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Is main frame:</td>
                    <td colspan="2">
                        <img width="16" height="16" src="/imagesNew/bool-force-{{process.isMainFrame}}.png">
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">No deprecation:</td>
                    <td colspan="2">
                        <img width="16" height="16" src="/imagesNew/bool-{{process.noDeprecation}}.png">
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Process ID:</td>
                    <td colspan="2">
                        {{process.pid}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">pp ID:</td>
