<!DOCTYPE html>
<html>

<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<meta http-equiv="pragma" content="no-cache">
	<meta http-equiv="Expires" content="wed, 26 Feb 1997 08:21:57 GMT">
	<link href="/stylesNew/css-vars.css" rel="stylesheet" type="text/css">
	<link href="/stylesNew/css_main.css" rel="stylesheet" type="text/css">
	<script language="javascript" src="/scripts/commonNew.js" type="text/javascript"></script>
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
</head>

<body onload="LoadHelp(window); resize(this);" onresize="resize(this);" marginwidth="0"
	marginheight="0">
   	<button class="show-hide-part-button" onclick="parent.ToggleHelpPanel(this)">&lt;</button>

    <h1>Account</h1>

	<div class="container loading-effect">
        <img src="/archive/data/user/avatar.png" width="64" height="64" title="Avatar" alt="Avatar" style="display: inline-block; transform: translateY(16px); border-radius: 50%;">
        <span style="font-size: 32px; font-weight: bold;" title="Username"><span>{{userData.username}}</span>#<span style="opacity: 60%;" title="Discriminator">{{userData.discriminator}}</span></span>
        <table style="width: 100%;">
            <tbody>
                <tr>
                    <td width="25%" class="Item">ID:</td>
                    <td colspan="2">
                        {{userData.id}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">Email:</td>
                    <td colspan="2">
                        {{userData.email}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">verified:</td>
                    <td colspan="2">
                        {{userData.verified}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">avatar_hash:</td>
                    <td colspan="2">
                        {{userData.avatar_hash}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">has_mobile:</td>
                    <td colspan="2">
                        {{userData.has_mobile}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">needs_email_verification:</td>
                    <td colspan="2">
                        {{userData.needs_email_verification}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">flags:</td>
                    <td colspan="2">
                        {{userData.flags}}
                    </td>
                </tr>
                <tr>
                    <td width="25%" class="Item">locale:</td>
                    <td colspan="2">
                        {{userData.settings.locale}}
                    </td>
                </tr>
            </tbody>
        </table>
	</div>

	<div class="container loading-effect">
        <h2>relationships</h2>
        <table class="datas" border="1" align="center" width="100%" cellspacing="0" cellpadding="0">
            <tbody>
                <tr>
                    <td class="ListM" style="width: 100%;"></td>
                    <td class="ListM">
                        <b>ID</b>
                    </td>
                </tr>
                {{#each userData.relationships}}
                <tr>
                    <td style="padding:0 4px 4px 4px;">
                        <div style="display: inline-block; transform: translate(-2px, 4px);">
                            {{#if avatarUrlSmall}}
                            <div class="tooltip">
                                <img src="{{this.avatarUrlSmall}}" width="16" height="16" title="Avatar" style="border-radius: 8px;">
                                <span class="tooltiptext">
                                    <img src="{{this.avatarUrlBig}}">
                                </span>
                            </div>
                            {{/if}}
                        </div>

                        <span title="Username">{{this.user.username}}</span><span style="color: #999;" title="Discriminator">#{{this.user.discriminator}}</span>
                        
                        {{#if bot}}<span class="tag">BOT</span>{{/if}}
                        {{#if system}}<span class="tag">System</span>{{/if}}
                        {{#if flags.HypeSquadOnlineHouse1}}
                        <span class="user-badge">
                            <img src="https://static.wikia.nocookie.net/discord/images/3/31/Hypesquad_bravery_badge.png">
                        </span>
                        {{/if}}

                        {{#if flags.HypeSquadOnlineHouse2}}
                        <span class="user-badge">
                            <img src="https://static.wikia.nocookie.net/discord/images/2/27/Hypesquad_brilliance_badge.png">
                        </span>
                        {{/if}}

                        {{#if flags.HypeSquadOnlineHouse3}}
                        <span class="user-badge">
                            <img src="https://static.wikia.nocookie.net/discord/images/c/ca/Hypesquad_balance_badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if flags.Staff}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Discord-staff.png">
                        </span>
                        {{/if}}
                        
                        {{#if flags.BugHunterLevel1}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Bug_hunter_badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if flags.BugHunterLevel2}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Bug_buster_badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if flags.CertifiedModerator}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Discord_Certified_Moderator.png">
                        </span>
                        {{/if}}
                        
                        {{#if flags.Partner}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/DiscordPartnerBadge.svg">
                        </span>
                        {{/if}}
                        
                        {{#if false}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Early_supporter_badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if false}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/HypeSquad_Event_Badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if false}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/New_badge.svg">
                        </span>
                        {{/if}}
                        
                        {{#if false}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Nitro_badge.png">
                        </span>
                        {{/if}}
                        
                        {{#if false}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/SupportsCommandsBadge.png">
                        </span>
                        {{/if}}
                        
                        {{#if VerifiedDeveloper}}
                        <span class="user-badge">
                            <img src="../imagesNew/userBadges/Verified_developer_badge.png">
                        </span>
                        {{/if}}

                        <div style="
                            width: 16px;
                            height: 16px;
                            border: 1px solid black;
                            border-radius: 3px;
                            background-color: {{this.hexAccentColor}};
                            float: right;
                            transform: translateY(4px);
                        " title="Accent color">
                    </td>
                    <td>
                        {{this.id}}
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
	</div>

    <script src="/scripts/loadEffect.js"></script>
</body>

</html>