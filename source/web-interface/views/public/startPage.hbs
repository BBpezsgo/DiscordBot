    <style>
        .tabcontent > div:not(.content-header) {
            pointer-events: none;
            opacity: 0;
            transition: transform 100ms, opacity 100ms;
            transform: translateX(-80px);
        }

        .tabcontent.tabcontent-loaded > div:not(.content-header) {
            pointer-events: all;
            opacity: 1;
            transform: translateX(0px);
        }

        .tabcontent > div.content-header > * {
            opacity: 0;
            transition: opacity 100ms;
        }

        .tabcontent.tabcontent-loaded > div.content-header > * {
            opacity: 1;
        }

        .sidebar {
            background-color: #2f3136;
            z-index: 1;
            overflow-x: hidden;
            overflow-y: scroll;
            height: 100%;
            margin: 0;
            padding: 0 !important;
        }

        .sidebar::-webkit-scrollbar {
            width: 4px;
        }
        .sidebar::-webkit-scrollbar-track {
            background: transparent;
        }
        .sidebar::-webkit-scrollbar-thumb {
            background: #202225;
            border-radius: 2px;
        }
        .sidebar::-webkit-scrollbar-thumb:hover {
            background: #202225;
        }

        #content {
            padding: 0;
            margin: 0;
        }

        html, body, #content, #content > .row {
            height: 100%;
        }

        .sidebar-buttons {
            list-style: none;
            padding: 0;
            margin: 15px 0 0 20px;
            width: calc(100% - 30px);
        }

        .sidebar-buttons > li {
            border-radius: 4px;
            padding: 4px 5px;
            margin: 2px 10px;
            height: 32px;
            font-weight: 600;
            cursor: pointer;
        }

        .sidebar-buttons > li:hover {
            background-color: #393c41;
        }

        .sidebar-buttons > li:active {
            background-color: #3f4248;
        }

        .toast-alert {
            background: #393c4190;
            border: 1px solid #3f4248;
            box-shadow: #0006 0px 0px 3px;
            border-radius: 5px;
            
            padding: 10px 15px;
            margin: 20px 5px 0px 15px;
            transition: opacity 500ms, transform 500ms !important;
        }

        .toast-alert.submitted {
            opacity: 0 !important;
            transform: translateX(-100px);
            pointer-events: none;
        }

        .toast-alert > h1 {
            display: inline-block;
            margin: 0px 0px 5px 0px;
            transform: translateY(10px);
            font-size: 16px;
            font-weight: 600;
        }

        .toast-alert > img {
            display: inline-block;
            width: 32px;
            height: 32px;
        }

        .toast-alert > small.success {
            font-variant: small-caps;
            font-weight: bold;
            display: block;
            margin: 0px;
            color: #46c46e;
        }

        .toast-alert input[type=submit] {
            border: none;
            outline: none;
            border-radius: 3px;
            background-color: #2d7d46;
            color: #fff;
            font-weight: 600;
            text-align: center;
            min-height: 32px;
            font-size: 14px;
            text-align: center;
            width: 100%;
            margin: 15px 0px 0px 0px;
            transition: background-color 200ms;
        }

        .toast-alert input[type=submit]:hover {
            background-color: #215b32;
        }

        .toast-alert input[type=submit]:active {
            background-color: #1e532e;
        }

        .sidebar .user-panel {
            background-color: #292b2f;
            padding: 5px 20px 20px 20px;
        }

        .container {
            max-width: calc(100% - 13px);
        }

        .content-header {
            background-color: #363a40;
            width: 100%;
            min-height: 50px;
        }

        .btn2 {
            border: 0;
            outline: 0;
            background-color: transparent;
            opacity: 50%;
        }

        .btn2:hover {
            opacity: 75%;
        }

        .btn2:active {
            opacity: 100%;
        }
    </style>

</head>

